{
    "name": "phirecms/phirecms",
    "description": "Phire CMS, a robust content management system written in PHP",
    "keywords": [
        "content management system",
        "cms",
        "phire",
        "phire cms",
        "php cms"
    ],
    "homepage": "http://www.phirecms.org/",
    "license": "New BSD",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "popphp/popphp": "2.*@dev",
        "popphp/pop-acl": "2.*@dev",
        "popphp/pop-archive": "2.*@dev",
        "popphp/pop-auth": "2.*@dev",
        "popphp/pop-db": "2.*@dev",
        "popphp/pop-form": "2.*@dev",
        "popphp/pop-ftp": "2.*@dev",
        "popphp/pop-http": "2.*@dev",
        "popphp/pop-mail": "2.*@dev",
        "popphp/pop-nav": "2.*@dev",
        "popphp/pop-paginator": "2.*@dev",
        "popphp/pop-view": "2.*@dev",
        "popphp/pop-web": "2.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Phire\\": "phire-cms/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "vendor-dir": "phire-cms/vendor"
    },
    "minimum-stability": "dev"
}
